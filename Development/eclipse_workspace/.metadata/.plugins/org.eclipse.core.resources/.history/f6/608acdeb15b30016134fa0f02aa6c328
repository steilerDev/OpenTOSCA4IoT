package de.tosca4iot.smarthome.Weather_Sensor.ConfigLoader;

import java.io.IOException;
import java.net.URL;

public class ReadConfigFileMain {
	public static void main(String[] args) throws IOException {
		
		URL location = ReadConfigFileMain.class.getProtectionDomain().getCodeSource().getLocation();
        System.out.println(location.getFile());
		
		GetPropertyValues properties = new GetPropertyValues();
		System.out.println(properties.getPropValues());
	}
}
